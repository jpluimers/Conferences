        <source>
          <![CDATA[unit Main;

interface

uses
  w3system, w3ctrls, w3graphics, w3components, w3forms, w3application, w3touch,
  w3dictionaries;

const
  CMaxFingers = 10;

type
  TPaintBoxForm = class(TW3form)
  private
    FCanvas: TW3GraphicControl;
    FColors: array [0..CMaxFingers-1] of TColor;
    FMouseDown: boolean;
    FMouseID: integer;
  protected
    procedure InitializeObject; override;
    procedure FinalizeObject; override;
    procedure HandleMouseDown(Sender: TObject; button: TMouseButton; Shift: TShiftState; X, Y: integer);
    procedure HandleMouseMove(Sender: TObject; Shift: TShiftState; X, Y: integer);
    procedure HandleMouseUp(Sender: TObject; button: TMouseButton; Shift: TShiftState; X, Y: integer);
    procedure HandleTouch(Sender: TObject; info: TW3TouchData);
    procedure Paint(Sender: TObject; canvas: TW3Canvas);
    procedure PaintFinger(id, X, Y: integer);
    procedure RandomizeColors;
    procedure Resize; override;
    procedure StyleTagObject; override;
  end;

implementation

procedure TPaintBoxForm.FinalizeObject;
begin
  FCanvas.Free;
  inherited;
end;

procedure TPaintBoxForm.InitializeObject;
begin
  inherited;
  FCanvas := TW3GraphicControl.Create(Self);
  FCanvas.OnPaint := Paint;
  FCanvas.OnMouseDown := HandleMouseDown;
  FCanvas.OnMouseMove := HandleMouseMove;
  FCanvas.OnMouseUp := HandleMouseUp;
  FCanvas.OnTouchBegin := HandleTouch;
  FCanvas.OnTouchMove := HandleTouch;
  FCanvas.OnTouchEnd := HandleTouch;
end;

procedure TPaintBoxForm.HandleMouseDown(Sender: TObject; button: TMouseButton; Shift: TShiftState; X, Y: integer);
begin
  FMouseDown := True;
  PaintFinger(FMouseID, X, Y);
end;

procedure TPaintBoxForm.HandleMouseMove(Sender: TObject; Shift: TShiftState; X, Y: integer);
begin
  if FMouseDown then 
    PaintFinger(FMouseID, X, Y);
end;

procedure TPaintBoxForm.HandleMouseUp(Sender: TObject; Button: TMouseButton; Shift: TShiftState; X, Y: integer);
begin
  FMouseDown := False;
  Inc(FMouseID);
end;

procedure TPaintBoxForm.HandleTouch(Sender: TObject; Info: TW3TouchData);
var
  i: Integer;
begin
  for i := 0 to Info.Touches.Count - 1 do
    PaintFinger(Info.Touches[i].Identifier, Info.Touches[i].ClientX, Info.Touches[i].ClientY);
end;

procedure TPaintBoxForm.Paint(Sender: TObject; Canvas: TW3Canvas);
begin
  RandomizeColors;
  FCanvas.Canvas.FillStyle := '#FFFFFF';
  FCanvas.Canvas.FillRectF(0, 0, Width, Height);
end;

procedure TPaintBoxForm.PaintFinger(ID, X, Y: Integer);
begin
  FCanvas.Canvas.BeginPath;
  FCanvas.Canvas.ArcF(X, Y, 5, 0, PI * 2, False);
  FCanvas.Canvas.ClosePath;
  FCanvas.Canvas.StrokeStyle := ColorToWebStr(FColors[id mod CMaxFingers]);
  FCanvas.Canvas.Stroke;
  FCanvas.Canvas.FillStyle := FCanvas.Canvas.StrokeStyle;
  FCanvas.Canvas.Fill;
end;

procedure TPaintBoxForm.RandomizeColors;
var
  i: Integer;
begin
  for i := 0 to CMaxFingers - 1 do
    FColors[i] := RGBToColor(RandomInt($FF), RandomInt($FF), RandomInt($FF));
end;

procedure TPaintBoxForm.Resize;
begin
  FCanvas.SetBounds(0, 0, Width, Height);
  FCanvas.Invalidate;
end;

procedure TPaintBoxForm.StyleTagObject;
begin
  inherited;
  StyleClass := 'TW3CustomForm';
end;
 
end.
]]>
