          <![CDATA[unit MultiFingerPaint;

interface

uses
  w3system, w3forms, w3application, Main;

type
  TApplication = class(TW3CustomApplication)
  private
    FForm: TW3CustomForm;
  public
    procedure ApplicationStarting;override;
  end;

implementation

{ TApplication }

procedure TApplication.ApplicationStarting;
begin
  //Add code above this line 
  FForm := TPaintBoxForm.Create(Display.View);
  FForm.Name := 'MultiPaint';
  RegisterFormInstance(FForm, True);
  inherited;
end;

end.
